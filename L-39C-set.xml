      		<airconditioning-type type="int">0</airconditioning-type><!-- cold for 12s: -1 auto: 0 warm for 12s: 1 -->
      		<airconditioning-enabled type="bool">true</airconditioning-enabled>
      		<airconditioning-temperature type="double">16.0</airconditioning-temperature><!-- used for auto -->
    </ventilation>

	<!-- Power switches -->
	<power>
		<battery type="bool">true</battery>
      	<fuelpump0 type="bool">false</fuelpump0>
      	<fuelpump1 type="bool">false</fuelpump1>
      	<rsbn type="bool">true</rsbn>
	</power>

	<electric>
		<battery-switch type="bool">false</battery-switch>
        
        <engine>
            <generator type="bool">false</generator>
        </engine>
	</electric>

	<gear>
		<brake-parking type="bool">true</brake-parking>
	</gear>

	<gunsight>
		<gunsight-norm>0.8</gunsight-norm>
        <gunsight-pos>1</gunsight-pos>
		<gunsight-dep>-2.3</gunsight-dep>
	</gunsight>  

	<armament>
		<trigger type="bool">false</trigger>
		<trigger1 type="bool">false</trigger1>
		<trigger2 type="bool">false</trigger2>
		<pylon-outer-sel type="bool">true</pylon-outer-sel>
		<pylon-inner-sel type="bool">false</pylon-inner-sel>
		<guns-sel type="bool">false</guns-sel>
		<master-arm type="bool">false</master-arm>
		<salvo-ripple type="double">0</salvo-ripple>
		<bomb-mode type="double">0</bomb-mode>
		<station n="0">
			<release type="bool">false</release>
		</station>
		<station n="1">
			<release type="bool">false</release>
		</station>
		<station n="2">
			<release type="bool">false</release>
		</station>
		<station n="3">
			<release type="bool">false</release>
		</station>
		<station n="4">
			<release type="bool">false</release>
		</station>
	</armament>

	<instrumentation>

		<danger-altitude-ft type="double">0.0</danger-altitude-ft>
		
		<transponder>
			<inputs>
				<serviceable type="bool">true</serviceable>
			</inputs>
		</transponder>
        
		<nav n="0">
			<serviceable type="bool">1</serviceable>
		</nav>
        
		<comm n="0">
			<serviceable type="bool">1</serviceable>
		</comm>

	</instrumentation>

	<navigation>
			<target-range type="double">0.0</target-range>
