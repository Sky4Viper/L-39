</controls>

<consumables>
	<fuel>
		<tank n="0">
			<tank-num type="int">0</tank-num>
			<level-gal_us archive="y">290</level-gal_us>
		</tank>

		<tank n="1">
			<tank-num type="int">0</tank-num>
			<level-gal_us archive="y">26</level-gal_us>
		</tank>

		<tank n="2">
			<tank-num type="int">0</tank-num>
			<level-gal_us archive="y">26</level-gal_us>
		</tank>

		<tank n="3">
			<name>Left Drop Tank</name>
			<selected type="bool">false</selected>
		</tank>
			
		<tank n="4">
			<name>Right Drop Tank</name>
			<selected type="bool">false</selected>
		</tank>
	</fuel>
