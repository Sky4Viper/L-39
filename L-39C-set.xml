<engines>
	<engine n="0">
		<running>false</running>
	</engine>
</engines>

<canopy>
    <position-norm type="float">1.0</position-norm>
</canopy>

<input>
	<keyboard>

		<key n="60">
        	<name>&lt;</name>
        	<desc>Select Outer Pylons</desc>
        	<binding>
				<command>property-toggle</command>
				<property>controls/armament/pylon-outer-sel</property>
			</binding>
    	</key>
       
		<key n="78">
			<name>N</name>
			<desc>Extend speedbrake</desc>
			<binding>
				<command>property-adjust</command>
				<property>/controls/flight/speedbrake</property>
				<step type="double">0.2</step>
				<min type="double">0</min>
				<max type="double">1</max>
			</binding>
		</key>

		<key n="81">
			<name>Shift-Q</name>
			<desc>Fuel Cutoff Switch or Lever.</desc>
			<binding>
				<condition>
					<property>/sim/input/selected/engine[0]</property>
				</condition>
				<command>property-toggle</command>
				<property>/controls/engines/engine[0]/cutoff</property>
			</binding>
		</key>
       
		<key n="99">
			<name>c</name>
			<desc>Toggle canopy</desc>
			<binding>
				<command>nasal</command>
				<script>canopy.canopy_switch(2)</script>
                <condition>
                    <not>
                        <property>/controls/canopy/lock</property>
                    </not>
                </condition>
			</binding>
		</key>

		<key n="100">
				<name>d</name>
				<desc>Fire selected weapons</desc>
				<binding>
					<command>property-assign</command>
					<property>controls/armament/trigger</property>
					<value>true</value>
				</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/armament/trigger</property>
					<value>false</value>
				</binding>
			</mod-up>
		</key>
       
		<key n="101">
			<name>e</name>
			<desc>Jettison Pylons</desc>
			<binding>
      			<command>nasal</command>
      			<script>weapons.jettison_outer()</script>
              	<condition>
                	<property>l-39za/controls/jett2swcover</property>
              	</condition>
			</binding>
		</key>

		<key n="102">
			<name>f</name>
			<desc>Launch sygnal flare</desc>
			<binding>
				<command>nasal</command>
      			<script>weapons.fire_FL()</script>
				<condition>
					<equals>
         				<property>sim/multiplay/generic/int[16]</property>
    					<value>1</value>
        			</equals>
				</condition>
			</binding>
			<mod-up>
				<binding>
					<command>nasal</command>
      				<script>weapons.stop_FL()</script>
				</binding>
			</mod-up>
		</key>

		<key n="109">
        	<name>m</name>
        	<desc>Masterarm</desc>
        	<binding>
            	<command>nasal</command>
            	<script><![CDATA[
                		setprop("controls/armament/master-arm",!getprop("controls/armament/master-arm"));
                		screen.log.write("Master-arm "~(getprop("controls/armament/master-arm")==0?"OFF":"ON"), 0.5, 0.5, 1);
                		]]>
				</script>
        	</binding>
			<binding>
          			<command>nasal</command>
          			<script>switchclicksound.click("switch")</script>
        	</binding>
    	</key>
       
		<key n="110">
			<name>n</name>
			<desc>Retract speedbrake</desc>
			<binding>
				<command>property-adjust</command>
				<property>/controls/flight/speedbrake</property>
				<step type="double">-0.2</step>
				<min type="double">0</min>
				<max type="double">1</max>
			</binding>
		</key>

		<key n="113">
        	<name>q</name>
        	<desc>Signal Off/Flare/Smoke </desc>
			<binding>
        		<command>property-adjust</command>
        		<property>sim/multiplay/generic/int[16]</property>
        		<step>1.0</step>
        		<min>0</min>
        		<max>3</max>
        		<wrap>true</wrap>
      		</binding>
			<binding>
          		<command>nasal</command>
          		<script>switchclicksound.click("switch")</script>
